<h4>商品一覧</h4>




<div class="col-xs-9">
	<table class="table table-hover">
		<h1>Users</h1>
		<thead>
			<tr>
				<th>商品ID</th>
				<th>商品名</th>
				<th>税抜き価格</th>
				<th>ジャンル</th>
				<th>販売ステータス</th>

			</tr>
		</thead>
		<tbody>
			<% @sweet.each do |sweet| %>
			<tr>
	        	<td><%= sweet.id %></td>
	            <td><%= sweet.name %></td>
	            <td><%= sweet.price %></td>
	            <td>ジャンル</td>
	            <td><%= sweet.is_soldout %></td>
	        </tr>
		    <% end %>
		</tbody>
	</table>
	</div>